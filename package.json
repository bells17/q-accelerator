{
  "name": "q-accelerator",
  "version": "1.0.0",
  "description": "Qiitaでの活動を加速させるChrome拡張機能",
  "scripts": {
    "clean": "rimraf extension",
    "build": "npm-run-all lint test clean build:copy build:webpack",
    "build:copy": "cpx \"./src/assets/**\" \"./extension/assets/\" && cpx \"./src/background/background.html\" \"./extension/background/\" && cpx \"./src/manifest.json\" \"./extension/\"",
    "build:webpack": "webpack",
    "lint": "eslint test src --fix",
    "test": "npm-run-all test:mocha",
    "test:mocha": "mocha --compilers js:babel-register --reporter nyan test/**/*.js",
    "watch": "npm-run-all clean build:copy watch:webpack",
    "watch:test": "npm-run-all 'test:mocha -- --watch'",
    "watch:webpack": " webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howdy39/q-tool.git"
  },
  "keywords": [
    "chrome-extension",
    "qiita"
  ],
  "author": "Tatsuya Nakano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howdy39/q-tool/issues"
  },
  "homepage": "https://github.com/howdy39/q-tool#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.8.1",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "postcss-loader": "^1.2.1",
    "postcss-nesting": "^2.3.1",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "clipboard": "^1.5.16",
    "moment": "^2.16.0",
    "vue": "^1.0.28",
    "vue-mdl": "^0.10.0",
    "vue-moment": "^2.0.2"
  }
}
