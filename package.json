{
  "name": "q-accelerator",
  "version": "1.0.0",
  "description": "Qiitaでの活動を加速させるChrome拡張機能",
  "scripts": {
    "clean": "rimraf extension dest",
    "lint": "eslint test src --fix",
    "test": "npm-run-all test:mocha",
    "test:mocha": "mocha --compilers js:babel-register test/**/*.js",
    "build": "npm-run-all lint test build:webpack build:copy",
    "build:babel": "npm-run-all clean && babel src --out-dir dest/src && babel test --out-dir dest/test",
    "build:webpack": "webpack",
    "build:copy": "cpx \"./src/assets/**\" \"./extension/assets/\" && cpx \"./src/lib/**\" \"./extension/lib/\" && cpx \"./src/manifest.json\" \"./extension/\"",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howdy39/q-tool.git"
  },
  "keywords": [
    "chrome-extension",
    "qiita"
  ],
  "author": "Tatsuya Nakano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/howdy39/q-tool/issues"
  },
  "homepage": "https://github.com/howdy39/q-tool#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "cpx": "^1.5.0",
    "eslint": "^3.8.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-webpack": "^1.5.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "moment": "^2.16.0",
    "vue": "^1.0.28",
    "vue-mdl": "^0.10.0",
    "vue-moment": "^2.0.2"
  }
}
